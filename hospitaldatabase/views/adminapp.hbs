                                <p style="color:red;">								</p>
                                <table class="table table-hover" id="sample-table-1">
                                    <thead style="font-family: Arial;">
                                        <tr>
                                            <th class="center">#</th>
                                            <th class="hidden-xs">Patient Name</th>
                                            <th>Doctor</th>
                                            <th>Spec</th>
                                            <th>Appointment Date</th>
                                            <th>Time</th>
                                            <th>Branch</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                    {{#each items}}
        <tr>
            <td>{{@index}}</td>
            <td>{{this.pname}}</td>
            <td>{{this.dname}}</td>
            <td>{{this.spec}}</td>
            <td>{{this.date}}</td>
            <td>{{this.time}}</td>
            <td>{{this.branch}}</td>
        </tr>
		{{/each}}
                                </table>
                            </div>
                        </div>
